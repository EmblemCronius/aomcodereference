<!-- Define a horizontal scrollbar. -->
<!ELEMENT horizscrollbar EMPTY >
<!ATTLIST horizscrollbar background 				CDATA #IMPLIED > <!-- transparent border if no background file is provided -->
<!ATTLIST horizscrollbar sliderareaart 				CDATA #IMPLIED > <!-- plain white background if no SliderAreaArt file is provided -->
<!ATTLIST horizscrollbar sliderbuttons 				CDATA #IMPLIED > <!-- if declared, shows the left/right buttons -->
<!ATTLIST horizscrollbar sliderleftbtnset 			CDATA #IMPLIED > <!-- only required when the sliderbuttons attribute is declared, typically "sliderLeftBtn" -->
<!ATTLIST horizscrollbar sliderpipart 				CDATA #IMPLIED > <!-- transparent if no SliderPipArt is provided -->
<!ATTLIST horizscrollbar sliderpiptexturecoords 	CDATA #IMPLIED > <!-- typically "0 0 1 1" -->
<!ATTLIST horizscrollbar sliderrightbtnset 			CDATA #IMPLIED > <!-- only required when the sliderbuttons attribute is declared, typically "sliderRightBtn" -->

<!-- Root element. Every layout document should start with 1 of these. -->
<!ELEMENT layout ( gadget* , position?, gadget* ) >

<!-- Define a listitem. This is typically encapsulated in a "scrollList" gadget. It's basically a fancy dropdownlist. -->
<!ELEMENT listitem ( #PCDATA ) >

<!-- Define the minimap position. -->
<!ELEMENT position EMPTY >
<!ATTLIST position name 	ID 		#REQUIRED >
<!ATTLIST position size1024 CDATA 	#REQUIRED >

<!-- Define 1 of the four states (default, clicked, rollover, ???) of a "stateButton" gadget. -->
<!ELEMENT stateentry EMPTY >
<!ATTLIST stateentry background 		CDATA #IMPLIED > <!-- you must define either a background image, -->
<!ATTLIST stateentry backgroundcolor 	CDATA #IMPLIED > <!-- or a solid background color in RGB. -->

<!-- Define a menu entry of a "menu" gadget. -->
<!ELEMENT submenu ( #PCDATA | submenu )* >		  <!-- the #PCDATA mentioned here in a Console Command -->
<!ATTLIST submenu accelerator 	CDATA #IMPLIED  > <!-- provide a hotkey for quick access to this feature -->
<!ATTLIST submenu configindex 	CDATA #IMPLIED  > <!-- shortcut for Console Command: configToggle(configName) -->
<!ATTLIST submenu disabled 		CDATA #IMPLIED  > <!-- if declared, this submenu option is not clickable -->
<!ATTLIST submenu separator 	CDATA #IMPLIED  > <!-- if declared, this submenu acts as a separator -->
<!ATTLIST submenu title 		CDATA #REQUIRED > <!-- define the submenu text -->

<!-- Define a vertical scrollbar. -->
<!ELEMENT vertscrollbar EMPTY >
<!ATTLIST vertscrollbar background 				CDATA #IMPLIED  > <!-- transparent border if no background file is provided, typically "ui\ui background yellow opaque" -->
<!ATTLIST vertscrollbar sliderareaart 			CDATA #IMPLIED  > <!-- plain white background if no SliderAreaArt file is provided -->
<!ATTLIST vertscrollbar sliderbuttons 			CDATA #IMPLIED  > <!-- if declared, shows the up/down buttons -->
<!ATTLIST vertscrollbar sliderdownbtnset 		CDATA #IMPLIED  > <!-- only required when the sliderbuttons attribute is declared, typically "sliderDownBtn" -->
